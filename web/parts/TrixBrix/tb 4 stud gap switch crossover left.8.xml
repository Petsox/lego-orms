<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>Dr. Matthias Runte, www.mattzobricks.com</Author>
	<Description>
		<en>TrixBrix 4 Stud Gap Single Crossover Switch Left</en>
		<de>TrixBrix Gleiswechselweiche links (4 Noppen Gleisabstand)</de>
	</Description>
	<ImageURL>https://trixbrix.eu/en_US/p/Left-4-Stud-Gap-Single-Crossover/785</ImageURL>
	<SortingKey>TrixBrix - Y37A</SortingKey>
	
	<SnapMargin>
		<left>1</left>
		<right>0</right>
		<top>0.5</top>
		<bottom>0.5</bottom>
	</SnapMargin>

	<ConnexionList>
		<!-- Peak -->
		<connexion>
			<type>1</type>
			<position>
				<x>2.5</x>
				<y>12</y>
			</position>
			<angle>90</angle>
			<nextConnexionPreference>1</nextConnexionPreference>
			<electricPlug>1</electricPlug>
		</connexion>
		<!-- Straight end -->
		<connexion>
			<type>1</type>
			<position>
				<x>2.5</x>
				<y>-12</y>
			</position>
			<angle>-90</angle>
			<nextConnexionPreference>0</nextConnexionPreference>
			<electricPlug>-1</electricPlug>
		</connexion>
		<!-- Turn end -->
		<connexion>
			<type>trixbrix_sw_4sg_single_crossover_left</type>
			<position>
				<x>-3.5</x>
				<y>-8</y>
			</position>
			<angle>-120</angle>
			<nextConnexionPreference>0</nextConnexionPreference>
			<electricPlug>-1</electricPlug>
		</connexion>

		<!-- Switch motor -->
		<connexion>
			<type>trixbrix_switchmotor</type>
			<position>
				<x>-4.5</x>
				<y>8.5</y>
			</position>
			<angle>180</angle>
		</connexion>
	</ConnexionList>
</part>
